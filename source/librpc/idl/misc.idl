#include "idl_types.h"

/*
  miscellaneous IDL structures
*/

interface misc
{
	typedef [public,noprint] struct {
		uint32 addr;
	} ipv4_addr;

	typedef [public,noprint,gensize] struct {
		uint32 time_low;
		uint16 time_mid;
		uint16 time_hi_and_version;
		uint8  clock_seq[2];
		uint8  node[6];
	} GUID;

	typedef [public] struct {
		uint32 handle_type;
		GUID   uuid;
	} policy_handle;
}
