#!/bin/sh
#
# $Id: swat.postrm,v 1.2.2.2 2002/05/02 20:20:28 jerry Exp $
#

case "$1" in
	purge)
		update-inetd --remove '/usr/sbin/swat$'
		;;
	remove)
		;;
	upgrade|failed-upgrade|abort-install|abort-upgrade|disappear)
		;;
	*)
		echo "$0: unknown action \"$1\""
		exit 0
		;;
esac

#DEBHELPER#

exit 0

