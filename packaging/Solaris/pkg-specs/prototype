#
# The prototype file for the Samba package.
#
# Information files.
# 
i pkginfo=./pkginfo
i copyright=./copyright
#
# Stuff that goes into the system areas of the filesystem.
#
d none /etc ? ? ?
d none /etc/init.d ? ? ?
f none /etc/init.d/samba.server=packaging/Solaris/pkg-specs/samba.server 0744 root sys
d none /etc/rc3.d ? ? ?
l none /etc/rc3.d/S95samba.server=/etc/init.d/samba.server
#
# Stuff put into the samba subtree. (Usually /usr/local/samba )
#
d none samba 0755 root other
d none samba/var 0755 root other
# 
# Binaries
#
d none samba/bin 0755 root other
f none samba/bin/smbd=source/bin/smbd 0755 root other
