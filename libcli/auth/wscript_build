	source='credentials.c session.c smbencrypt.c smbdes.c',
	public_deps='MSRPC_PARSE',
	public_headers='credentials.h:domain_credentials.h'
	)


bld.SAMBA_SUBSYSTEM('COMMON_SCHANNELDB',
	source='schannel_state_tdb.c',
	deps='tdb-wrap'
	)


