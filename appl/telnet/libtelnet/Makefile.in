# $Id$

srcdir		= @srcdir@
top_srcdir	= @top_srcdir@
VPATH		= @srcdir@

SHELL		= /bin/sh

CC = @CC@
AR = ar
RANLIB = @RANLIB@
DEFS = @DEFS@
CFLAGS = @CFLAGS@
LIBNAME = $(LIBPREFIX)telnet
LIBEXT = a
LIBPREFIX = @LIBPREFIX@
LIB = $(LIBNAME).$(LIBEXT)

prefix = @prefix@

SOURCES=auth.c encrypt.c genget.c enc_des.c misc.c kerberos.c read_password.c

OBJECTS=auth.o encrypt.o genget.o enc_des.o misc.o kerberos.o read_password.o

all: $(LIB)

libtop = @libtop@

.c.o:
	$(CC) -c $(CPPFLAGS) $(CFLAGS) $(DEFS) -I../../../include -I../../.. -I$(srcdir)/.. $<

$(LIB): $(OBJECTS)
	rm -f $@
	$(AR) cr $@ $(OBJECTS)
	-$(RANLIB) $@

install:
	@true

uninstall:
	@true

TAGS: $(SOURCES)
	etags $(SOURCES)

clean cleandir:
	rm -f *.o *.a \#* *~ core

distclean: clean
	rm -f Makefile *~
