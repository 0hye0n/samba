# Makefile.in,v 1.2 1994/05/13 05:02:46 assar Exp

srcdir		= @srcdir@
VPATH		= @srcdir@

SHELL		= /bin/sh

CC = @CC@
RANLIB = @RANLIB@
DEFS = @DEFS@
CFLAGS = @CFLAGS@

INSTALL = @INSTALL@
prefix = @prefix@
exec_prefix = $(prefix)
libdir = $(exec_prefix)/lib

SUBDIRS=libtelnet telnet telnetd

all:
	for i in $(SUBDIRS); \
	do (cd $$i; $(MAKE) $(MFLAGS) all); done

.PHONY: configure

configure: 
	$(srcdir)/configure

clean cleandir:
	for i in $(SUBDIRS); \
	do (cd $$i; $(MAKE) $(MFLAGS) clean); done

