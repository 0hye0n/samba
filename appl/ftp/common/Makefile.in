# 

srcdir		= @srcdir@
top_srcdir	= @top_srcdir@
VPATH		= @srcdir@

SHELL		= /bin/sh

CC 	= @CC@
RANLIB 	= @RANLIB@
DEFS 	= @DEFS@
CFLAGS 	= @CFLAGS@

INSTALL = @INSTALL@

prefix 	= @prefix@
exec_prefix = $(prefix)
libdir = $(exec_prefix)/lib

ATHENA = /usr/athena

libcommon_OBJS = base64.o glob.o @LIBOBJS@

all: libcommon.a

.c.o:
	$(CC) -c $(CFLAGS) -I. -I.. -I$(ATHENA)/include $(DEFS) $<

libcommon.a: $(libcommon_OBJS)
	ar cr libcommon.a $(libcommon_OBJS)
	$(RANLIB) libcommon.a


install:

clean cleandir:
	rm -f *~ *.o libcommon.a core \#*

distclean: 
	rm -f Makefile
	rm -rf CVS
