#!/usr/bin/env python

bld.SAMBA_LIBRARY('dsdb-module',
	source=[],
	deps='DSDB_MODULE_HELPERS DSDB_MODULE_HELPER_SCHEMA DSDB_MODULE_HELPER_RIDALLOC',
	private_library=True,
	grouping_library=True)

